{
  "name": "poao-dev-proxy",
  "version": "1.0.0",
  "description": "cors-proxy",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "prettier": "prettier --write 'src/**/*.ts'",
    "test": "jest"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write"
    ]
  },
  "importSort": {
    ".ts": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.2",
    "husky": "^7.0.1",
    "import-sort-style-module": "^6.0.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "prettier-plugin-import-sort": "0.0.7",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  }
}
